<h1>CitySights</h1>
<a href="/">Back to Homepage</a><br>

<% let count = 0; let flag = true%>
    <%var day1_len = ((day1a.length > day1b.length) ? day1a.length : day1b.length)
    var day2_len = ((day2a.length > day2b.length) ? day2a.length : day2b.length)
    var day3_len = ((day3a.length > day3b.length) ? day3a.length : day3b.length)%>
    <%for(let j = 0; j<day3_len && flag == true;j++){%>
        <%for(let k = 0; k<day2_len && flag == true;k++){%>
            <%for(let m = 0; m<day1_len && flag == true;m++){%>
                <br>
                <h3>Schedule <%= count+1 %></h3>
                <h5>Day 1:</h5>
                <% if(m < day1a.length){ %>
                    <%= day1a[m].Name %>
                    <br>
                <% } %>
                <% if(m < day1b.length){ %>
                    <%= day1b[m].Name %>
                <% } %>
                <br>
                <br>
                <h5>Day 2:</h5>
                <% if(k < day2a.length){ %>
                    <%= day2a[k].Name %>
                    <br>
                <% } %>
                <% if(k < day2b.length){ %>
                    <%= day2b[k].Name %>
                <% } %>
                <br>
                <br>
                <h5>Day 3:</h5>
                <% if(j < day3a.length){ %>
                    <%= day3a[j].Name %>
                    <br>
                <% } %>
                <% if(j < day3b.length){ %>
                    <%= day3b[j].Name %>
                <% } %>
                <br>
                <% count++ %>
                <% if(count>=10){ %>
                    <br><br>
                    <% flag = false; break; %>
                <% } %>
            <% }%>
        <% }%>
    <% }%>
